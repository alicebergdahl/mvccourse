{% extends 'base.html.twig' %}

{% block title %}Sorterad kortlek{% endblock %}

{% block body %}

<div class="content">
    <div class="row">
        <h1>Sorterad kortlek</h1>
    </div>
    
    <div class="row">
        <div class="column">
    <table border="1">
        {% for card in deck.getCards() %}
            <tr>
                <td>{{ card.getSuit() }}  {{ card.getValue() }}</td>
            </tr>
        {% endfor %}
    </table>

        </div>
    </div>
</div>
{% endblock %}
